{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "build:css": "postcss \"app/styles/*.scss\" \"app/routes/*/*/styles/*.scss\" --base app --dir app --ext css --env production",
    "dev": "remix dev",
    "dev:css": "postcss \"app/styles/*.scss\" \"app/routes/*/*/styles/*.scss\" --base app --dir app --ext css --verbose --watch",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "@remix-run/vercel": "^1.11.1",
    "@vercel/node": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.1.3",
    "remix-utils": "^6.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.11.1",
    "@remix-run/eslint-config": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.27.0",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-map-get": "^0.3.0",
    "postcss-mixins": "^9.0.4",
    "postcss-scss": "^4.0.6",
    "precss": "^4.0.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
